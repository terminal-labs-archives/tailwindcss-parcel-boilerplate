{
  "name": "tailwindcss-parcel-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hacknug/tailwindcss-parcel-boilerplate.git"
  },
  "scripts": {
    "postcss:watch": "postcss src/assets/css/*.css -o src/assets/css/index.css --env development -w",
    "postcss:build": "postcss src/assets/css/*.css --dir build --env production",
    "parcel:serve": "parcel src/index.html",
    "parcel:watch": "parcel watch src/index.html",
    "parcel:build": "parcel build src/index.html",
    "clean": "rimraf ./dist && rimraf ./.cache",
    "build": "npm-run-all -s clean postcss:build parcel:build",
    "start": "npm-run-all -s clean -p postcss:watch parcel:serve"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind css",
    "parcel",
    "postcss",
    "posthtml"
  ],
  "author": {
    "name": "Nestor Vera",
    "email": "nestorvera@me.com",
    "url": "https://nestor.rip/"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "codemirror": "^5.59.2",
    "datatables.net": "^1.10.23",
    "datatables.net-bs4": "^1.10.23",
    "datatables.net-buttons": "^1.6.5",
    "datatables.net-responsive": "^2.2.7",
    "jquery": "^3.5.1",
    "js-yaml": "^4.0.0",
    "moment": "^2.29.1",
    "parcel-bundler": "1.12.4",
    "quill": "^1.3.7",
    "quilljs-markdown": "^1.1.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "percy": "^0.28.6",
    "postcss": "^8.2.6",
    "postcss-cli": "^7.1.0",
    "postcss-fontpath": "^1.0.0",
    "posthtml-expressions": "^1.1.1",
    "posthtml-include": "^1.2.0",
    "posthtml-load-config": "^2.0.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.9.6"
  }
}
